<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wbluetooth.android" version="0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>wBluetooth</name>
    <description>
        wBluetooth LED Control for HC-05.
    </description>
    <author email="max30012540@hotmail.com" href="http://www.ioxhop.com/">
        IOXhop
    </author>
	<preference name="permissions" value="none" />
	<preference name="orientation" value="portrait" />
    <content src="index.html" />
    <access origin="*" />
	<gap:plugin name="com.megster.cordova.bluetoothserial" version="0.2.2" />
	<icon src="icon/ldpi.png" gap:platform="android" gap:density="ldpi" />
	<icon src="icon/mdpi.png" gap:platform="android" gap:density="mdpi" />
	<icon src="icon/hdpi.png" gap:platform="android" gap:density="hdpi" />
	<icon src="icon/xhdpi.png" gap:platform="android" gap:density="xhdpi" />
	<icon src="icon/xxhdpi.png" gap:platform="android" gap:density="xxhdpi" />
</widget>
